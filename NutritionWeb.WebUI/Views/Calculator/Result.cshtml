@model NutritionWeb.WebUI.Models.PatientViewModel
@{
    ViewBag.Title = "NB - Results";
}
@Html.Partial("TrackingCode");
<style>
    table, th, tr, td {
        border: 3px solid black;

        background-color: white;
        text-align: center;
    }
</style>
        <table class="table">

            @foreach (var dataPoint in Model.InputData)
            {
                <tr>
                    <th>@dataPoint.Heading</th>
                    <td>@dataPoint.Value1</td>
                    <td>@dataPoint.Value2</td>
                </tr>
            }

        </table>


        <table class="table">

                @foreach (var dataPoint in Model.Calculations)
                {
                    <tr>
                        <th>@dataPoint.Heading</th>
                        <td>@dataPoint.Value1</td>
                        <td>@dataPoint.Value2</td>
                    </tr>
                }
        </table>

    <a class="btn btn-lg btn-success btn-block" type="button" href="@Url.Action(@ViewBag.calculator, "Calculator")" id="back">Back</a>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/jquery-ui.min.js"></script>
<script src="~/Scripts/result.js"></script>

